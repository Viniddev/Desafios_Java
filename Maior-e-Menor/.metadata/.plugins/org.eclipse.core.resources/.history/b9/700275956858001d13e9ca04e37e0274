package maiorEMenor;
import java.util.Scanner;

public class MaiorEMenor {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		float maior, menor, numero;
		
		for(int n = 0; n < 10; n++) {
			System.out.print("Digite um nÃºmero");
			numero = sc.nextFloat();
			
			if(n == 1) {
				maior = numero;
				menor = numero;
			}
		}
	}
}
